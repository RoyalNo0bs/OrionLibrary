function OrionLib:MakeWindow(WindowValues)
	local TweenService = game:GetService("TweenService")
	local UIS = game:GetService("UserInputService")
	local Window_Name = WindowValues.Name
	local Window_HidePremium = WindowValues.HidePremium
	local Window_SaveConfig = WindowValues.SaveConfig
	local Window_ConfigFolder = WindowValues.ConfigFolder
	local plr = game.Players.LocalPlayer
	local WindowFunctions = {}
	
	local OrionGui = Instance.new("ScreenGui")
	local Orion = Instance.new("Frame")
	local Orion_UICorner = Instance.new("UICorner")
	local TopBar = Instance.new("Frame")
	local LibraryTitle = Instance.new("TextLabel")
	local TopBar_Options = Instance.new("Frame")
	local TopBar_UICorner = Instance.new("UICorner")
	local Options_Line = Instance.new("Frame")
	local Options_UIStroke = Instance.new("UIStroke")
	local CloseButton = Instance.new("TextButton")
	local CloseButton_Icon = Instance.new("ImageLabel")
	local MinimizeButton = Instance.new("TextButton")
	local MinimizeButton_Icon = Instance.new("ImageLabel")
	local TopBar_Line = Instance.new("Frame")
	local TabBar = Instance.new("Frame")
	local Tabs = Instance.new("ScrollingFrame")
	local Tabs_UIListLayout = Instance.new("UIListLayout")
	local Tabs_UIPadding = Instance.new("UIPadding")
	local ProfileFrame = Instance.new("Frame")
	local ProfileFrame_Boarder = Instance.new("Frame")
	local Profile = Instance.new("Frame")
	local ProfileFrame_UICorner = Instance.new("UICorner")
	local Profile_Username = Instance.new("TextLabel")
	local Profile_DisplayName = Instance.new("TextLabel")
	local Profile_Icon = Instance.new("Frame")
	local Profile_UICorner = Instance.new("UICorner")
	local Profile_BackDrop = Instance.new("ImageLabel")
	local ProfileIcon = Instance.new("ImageLabel")
	local UICorner_5 = Instance.new("UICorner")
	local TabBar_SideBoarder = Instance.new("Frame")
	local TabBar_Line = Instance.new("Frame")
	local TabBar_Boarder = Instance.new("Frame")
	local TopBarHitbox = Instance.new("Frame")

	OrionGui.Name = "Orion"
	OrionGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
	OrionGui.DisplayOrder = 999
	OrionGui.Enabled = true

	Orion.Name = "Orion"
	Orion.Parent = OrionGui
	Orion.AnchorPoint = Vector2.new(0.5, 0.5)
	Orion.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
	Orion.BorderSizePixel = 0
	Orion.ClipsDescendants = true
	Orion.Position = UDim2.new(0.374876857, 0, 0.595667899, 0)
	Orion.Size = UDim2.new(0, 615, 0, 344)
	Orion.AnchorPoint = Vector2.new(0.5,0)

	Orion_UICorner.CornerRadius = UDim.new(0, 10)
	Orion_UICorner.Parent = Orion

	TopBar.Name = "TopBar"
	TopBar.Parent = Orion
	TopBar.BackgroundTransparency = 1.000
	TopBar.Size = UDim2.new(1, 0, 0, 50)

	LibraryTitle.Name = "LibraryTitle"
	LibraryTitle.Parent = TopBar
	LibraryTitle.BackgroundTransparency = 1.000
	LibraryTitle.Position = UDim2.new(0, 25, 0, -24)
	LibraryTitle.Size = UDim2.new(1, -30, 2, 0)
	LibraryTitle.Font = Enum.Font.MontserratBlack
	LibraryTitle.TextColor3 = Color3.fromRGB(240, 240, 240)
	LibraryTitle.TextSize = 20.000
	LibraryTitle.TextXAlignment = Enum.TextXAlignment.Left

	TopBar_Options.Parent = TopBar
	TopBar_Options.BackgroundColor3 = Color3.fromRGB(32, 32, 32)
	TopBar_Options.BorderSizePixel = 0
	TopBar_Options.Position = UDim2.new(1, -90, 0, 10)
	TopBar_Options.Size = UDim2.new(0, 70, 0, 30)

	TopBar_UICorner.CornerRadius = UDim.new(0, 7)
	TopBar_UICorner.Parent = TopBar_Options

	Options_Line.Parent = TopBar_Options
	Options_Line.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
	Options_Line.BorderSizePixel = 0
	Options_Line.Position = UDim2.new(0.5, 0, 0, 0)
	Options_Line.Size = UDim2.new(0, 1, 1, 0)

	Options_UIStroke.Parent = TopBar_Options
	Options_UIStroke.Thickness = 1
	Options_UIStroke.Color = Color3.fromRGB(60, 60, 60)
	
	CloseButton.Parent = TopBar_Options
	CloseButton.BackgroundTransparency = 1.000
	CloseButton.BorderSizePixel = 0
	CloseButton.Position = UDim2.new(0.5, 0, 0, 0)
	CloseButton.Size = UDim2.new(0.5, 0, 1, 0)
	CloseButton.AutoButtonColor = false
	CloseButton.Text = ""
	CloseButton.TextColor3 = Color3.fromRGB(27, 42, 53)

	CloseButton_Icon.Name = "Icon"
	CloseButton_Icon.Parent = CloseButton
	CloseButton_Icon.BackgroundTransparency = 1.000
	CloseButton_Icon.Position = UDim2.new(0, 9, 0, 6)
	CloseButton_Icon.Size = UDim2.new(0, 18, 0, 18)
	CloseButton_Icon.Image = "rbxassetid://7072725342"
	CloseButton_Icon.ImageColor3 = Color3.fromRGB(240, 240, 240)

	MinimizeButton.Parent = TopBar_Options
	MinimizeButton.BackgroundTransparency = 1.000
	MinimizeButton.BorderSizePixel = 0
	MinimizeButton.Size = UDim2.new(0.5, 0, 1, 0)
	MinimizeButton.AutoButtonColor = false
	MinimizeButton.Text = ""

	MinimizeButton_Icon.Name = "Icon"
	MinimizeButton_Icon.Parent = MinimizeButton
	MinimizeButton_Icon.BackgroundTransparency = 1.000
	MinimizeButton_Icon.Position = UDim2.new(0, 9, 0, 6)
	MinimizeButton_Icon.Size = UDim2.new(0, 18, 0, 18)
	MinimizeButton_Icon.Image = "rbxassetid://7072719338"
	MinimizeButton_Icon.ImageColor3 = Color3.fromRGB(240, 240, 240)

	TopBar_Line.Parent = TopBar
	TopBar_Line.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
	TopBar_Line.BorderSizePixel = 0
	TopBar_Line.Position = UDim2.new(0, 0, 1, -1)
	TopBar_Line.Size = UDim2.new(1, 0, 0, 1)

	TabBar.Name = "TabBar"
	TabBar.Parent = Orion
	TabBar.BackgroundColor3 = Color3.fromRGB(32, 32, 32)
	TabBar.BorderSizePixel = 0
	TabBar.Position = UDim2.new(0, 0, 0, 50)
	TabBar.Size = UDim2.new(0, 150, 1, -50)

	Tabs.Name = "Tabs"
	Tabs.Parent = TabBar
	Tabs.BackgroundTransparency = 1
	Tabs.BorderSizePixel = 0
	Tabs.Size = UDim2.new(1, 0, 1, -50)
	Tabs.BottomImage = "rbxassetid://7445543667"
	Tabs.MidImage = "rbxassetid://7445543667"
	Tabs.ScrollBarThickness = 4
	Tabs.TopImage = "rbxassetid://7445543667"
	Tabs.ScrollBarImageColor3 = Color3.fromRGB(60, 60, 60)
	Tabs.ScrollBarImageTransparency = 0

	Tabs_UIListLayout.Parent = Tabs
	Tabs_UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder

	Tabs_UIPadding.Parent = Tabs
	Tabs_UIPadding.PaddingBottom = UDim.new(0, 8)
	Tabs_UIPadding.PaddingTop = UDim.new(0, 8)

	ProfileFrame.Name = "Profile"
	ProfileFrame.Parent = TabBar
	ProfileFrame.BackgroundTransparency = 1.000
	ProfileFrame.Position = UDim2.new(0, 0, 1, -50)
	ProfileFrame.Size = UDim2.new(1, 0, 0, 50)

	ProfileFrame_Boarder.Parent = ProfileFrame
	ProfileFrame_Boarder.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
	ProfileFrame_Boarder.BorderSizePixel = 0
	ProfileFrame_Boarder.Size = UDim2.new(1, 0, 0, 1)

	Profile.Parent = ProfileFrame
	Profile.AnchorPoint = Vector2.new(0, 0.5)
	Profile.BackgroundTransparency = 1.000
	Profile.Position = UDim2.new(0, 10, 0.5, 0)
	Profile.Size = UDim2.new(0, 32, 0, 32)

	ProfileFrame_UICorner.CornerRadius = UDim.new(1, 10)
	ProfileFrame_UICorner.Parent = Profile

	Profile_DisplayName.Name = "DisplayName"
	Profile_DisplayName.Parent = ProfileFrame
	Profile_DisplayName.BackgroundTransparency = 1.000
	Profile_DisplayName.ClipsDescendants = true
	Profile_DisplayName.Position = UDim2.new(0, 50, 0, 12)
	Profile_DisplayName.Size = UDim2.new(1, -60, 0, 13)
	Profile_DisplayName.Font = Enum.Font.GothamBold
	Profile_DisplayName.Text = "Icetea_Trash"
	Profile_DisplayName.TextColor3 = Color3.fromRGB(240, 240, 240)
	Profile_DisplayName.TextSize = 13.000
	Profile_DisplayName.TextXAlignment = Enum.TextXAlignment.Left

	Profile_Username.Name = "Username"
	Profile_Username.Parent = ProfileFrame
	Profile_Username.BackgroundTransparency = 1.000
	Profile_Username.Position = UDim2.new(0, 50, 1, -25)
	Profile_Username.Size = UDim2.new(1, -60, 0, 12)
	Profile_Username.Font = Enum.Font.Roboto
	Profile_Username.Text = plr.Name
	Profile_Username.TextColor3 = Color3.fromRGB(150, 150, 150)
	Profile_Username.TextSize = 12.000
	Profile_Username.TextXAlignment = Enum.TextXAlignment.Left

	ProfileIcon.Parent = Profile
	ProfileIcon.BackgroundTransparency = 1.000
	ProfileIcon.Size = UDim2.new(1, 0, 1, 0)
	ProfileIcon.Image = "https://www.roblox.com/headshot-thumbnail/image?userId=4955233520&width=420&height=420&format=png"

	ProfileIcon.Name = "ProfileIcon"
	ProfileIcon.Parent = Profile
	ProfileIcon.BackgroundTransparency = 1.000
	ProfileIcon.Size = UDim2.new(1, 0, 1, 0)
	ProfileIcon.Image = "rbxassetid://4031889928"
	ProfileIcon.ImageColor3 = Color3.fromRGB(32, 32, 32)

	Profile_BackDrop.Name = "Icon"
	Profile_BackDrop.Parent = ProfileFrame
	Profile_BackDrop.AnchorPoint = Vector2.new(0, 0.5)
	Profile_BackDrop.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
	Profile_BackDrop.BorderSizePixel = 0
	Profile_BackDrop.Position = UDim2.new(0, 10, 0.5, 0)
	Profile_BackDrop.Size = UDim2.new(0, 32, 0, 32)

	Profile_UICorner.CornerRadius = UDim.new(1, 10)
	Profile_UICorner.Parent = Profile_BackDrop


	UICorner_5.CornerRadius = UDim.new(0, 10)
	UICorner_5.Parent = TabBar
	
	TabBar_SideBoarder.Parent = TabBar
	TabBar_SideBoarder.BackgroundColor3 = Color3.fromRGB(32, 32, 32)
	TabBar_SideBoarder.BorderSizePixel = 0
	TabBar_SideBoarder.Position = UDim2.new(1, -10, 0, 0)
	TabBar_SideBoarder.Size = UDim2.new(0, 10, 1, 0)
	TabBar_SideBoarder.ZIndex = 0

	TabBar_Line.Parent = TabBar
	TabBar_Line.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
	TabBar_Line.BorderSizePixel = 0
	TabBar_Line.Position = UDim2.new(1, -1, 0, 0)
	TabBar_Line.Size = UDim2.new(0, 1, 1, 0)

	TabBar_Boarder.Parent = TabBar
	TabBar_Boarder.BackgroundColor3 = Color3.fromRGB(32, 32, 32)
	TabBar_Boarder.BorderSizePixel = 0
	TabBar_Boarder.Size = UDim2.new(1, 0, 0, 10)

	TopBarHitbox.Name = "TopBarHitbox"
	TopBarHitbox.Parent = Orion
	TopBarHitbox.BackgroundTransparency = 1.000
	TopBarHitbox.Size = UDim2.new(1, 0, 0, 50)

	Profile_Username.Text = plr.Name
	Profile_DisplayName.Text = plr.DisplayName
	LibraryTitle.Text = Window_Name

	MinimizeButton.MouseButton1Click:Connect(function()
		if Orion.Size == UDim2.new(0,615,0,344) then
			local tweenInfo = TweenInfo.new(
				0.1,
				Enum.EasingStyle.Linear,
				Enum.EasingDirection.Out,
				0,
				false,
				0
			)
			TweenService:Create(Orion,tweenInfo,{Size = UDim2.new(0,615,0,50)}):Play()
			ProfileFrame.Visible = false
			wait(0.1)
			for _, m in ipairs(Orion:GetChildren()) do
				if m:IsA("Frame") and m:IsA("ScrollingFrame") then
					m.Visible = false
				end
			end
			TopBar_Line.Visible = false
			TopBar.Visible = true
		else
			local tweenInfo = TweenInfo.new(
				0.1,
				Enum.EasingStyle.Linear,
				Enum.EasingDirection.Out,
				0,
				false,
				0
			)
			TweenService:Create(Orion,tweenInfo,{Size = UDim2.new(0,615,0,344)}):Play()
			TopBar_Line.Visible = true
			wait(0.1)
			for _, m in ipairs(Orion:GetChildren()) do
				if m:IsA("Frame") and m:IsA("ScrollingFrame") then
					m.Visible = true
				end
			end
			TopBar.Visible = true
			ProfileFrame.Visible = true
		end
	end)
	
	local dragging = false
	local offset = Vector2.new(0, 0)
	TopBar.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			offset = Orion.Position - UDim2.new(0, input.Position.X, 0, input.Position.Y)
		end
	end)
	Orion.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = false
		end
	end)
	game:GetService("UserInputService").InputChanged:Connect(function(input)
		if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
			Orion.Position = UDim2.new(0, input.Position.X, 0, input.Position.Y) + offset
		end
	end)
	
	table.insert(OrionLib,Orion)
	
	function WindowFunctions:MakeTab(Values)
		local Icon = Values.Icon
		local Name = Values.Name
		local PremiumOnly
		local TabFunctions = {}

		local TabButton = Instance.new("TextButton")
		local TabButton_Icon = Instance.new("ImageLabel")
		local TabButton_Title = Instance.new("TextLabel")
		local ItemContainer = Instance.new("ScrollingFrame")
		local ItemContainer_UIListLayout = Instance.new("UIListLayout")
		local ItemContainer_UIPadding = Instance.new("UIPadding")

		TabButton.Parent = Tabs
		TabButton.BackgroundTransparency = 1
		TabButton.Size = UDim2.new(1,0,0,30)
		TabButton.TextTransparency = 1
		TabButton.Name = Name

		TabButton_Title.Parent = TabButton
		TabButton_Title.BackgroundTransparency = 1
		TabButton_Title.Font = Enum.Font.MontserratMedium
		TabButton_Title.Text = Name
		TabButton_Title.Position = UDim2.new(0,35,0,0)
		TabButton_Title.Size = UDim2.new(1,-35,1,0)
		TabButton_Title.TextSize = 14
		TabButton_Title.TextColor3 = Color3.fromRGB(240, 240, 240)
		TabButton_Title.TextXAlignment = Enum.TextXAlignment.Left
		TabButton_Title.TextTransparency = 0.4

		ItemContainer.Name = Name
		ItemContainer.Parent = Orion
		ItemContainer.BackgroundTransparency = 1.000
		ItemContainer.BorderSizePixel = 0
		ItemContainer.Position = UDim2.new(0, 150, 0, 50)
		ItemContainer.Size = UDim2.new(1, -150, 1, -50)
		ItemContainer.Visible = false
		ItemContainer.BottomImage = "rbxassetid://7445543667"
		ItemContainer.CanvasSize = UDim2.new(0, 0, 0, 0)
		ItemContainer.MidImage = "rbxassetid://7445543667"
		ItemContainer.ScrollBarThickness = 5
		ItemContainer.TopImage = "rbxassetid://7445543667"
		ItemContainer.ScrollBarImageColor3 = Color3.fromRGB(60, 60, 60)
		ItemContainer.AutomaticCanvasSize = Enum.AutomaticSize.Y

		ItemContainer_UIListLayout.Parent = ItemContainer
		ItemContainer_UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
		ItemContainer_UIListLayout.Padding = UDim.new(0, 6)

		ItemContainer_UIPadding.Parent = ItemContainer
		ItemContainer_UIPadding.PaddingBottom = UDim.new(0, 15)
		ItemContainer_UIPadding.PaddingLeft = UDim.new(0, 10)
		ItemContainer_UIPadding.PaddingRight = UDim.new(0, 10)
		ItemContainer_UIPadding.PaddingTop = UDim.new(0, 15)

		TabButton_Icon.Parent = TabButton
		TabButton_Icon.Image = Icon
		TabButton_Icon.Size = UDim2.new(0,18,0,18)
		TabButton_Icon.Position = UDim2.new(0,10,0.5,0)
		TabButton_Icon.AnchorPoint = Vector2.new(0,0.5)
		TabButton_Icon.BackgroundTransparency = 1 
		TabButton_Icon.ImageTransparency = 0.4
		TabButton_Icon.Name = "Icon"

		TabButton.MouseButton1Click:Connect(function()
			for _, Tab in ipairs(Orion:GetChildren()) do
				if Tab:IsA("ScrollingFrame") then
					Tab.Visible = false
				end
			end
			for _, Tab in ipairs(Tabs:GetChildren()) do
				if Tab:IsA("TextButton") then
					Tab.TextLabel.TextTransparency = 0.4
					Tab.TextLabel.Font = Enum.Font.MontserratMedium
					Tab.Icon.ImageTransparency = 0.4
				end
			end
			
			ItemContainer.Visible = true
			TabButton_Title.Font = Enum.Font.MontserratBlack
			TabButton_Title.TextTransparency = 0
			TabButton_Title.ImageTransparency = 0
		end)

		function TabFunctions:AddToggle(ToggleValues)
			local ToggleFunctions = {}
			local Toggle = Instance.new("Frame")
			local UICorner_6 = Instance.new("UICorner")
			local Content1 = Instance.new("TextLabel")
			local UIStroke = Instance.new("UIStroke")
			local Frame = Instance.new("Frame")
			local UICorner_7 = Instance.new("UICorner")
			local Ico = Instance.new("ImageLabel")
			local TextButton = Instance.new("TextButton")

			Toggle.Name = "Toggle"
			Toggle.Parent = ItemContainer
			Toggle.BackgroundColor3 = Color3.fromRGB(32, 32, 32)
			Toggle.BorderSizePixel = 0
			Toggle.Size = UDim2.new(1, 0, 0, 38)

			UICorner_6.CornerRadius = UDim.new(0, 5)
			UICorner_6.Parent = Toggle

			UIStroke.Parent = Toggle
			UIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Contextual
			UIStroke.Color = Color3.fromRGB(60, 60, 60)
			UIStroke.Thickness = 1

			Content1.Name = "Content"
			Content1.Parent = Toggle
			Content1.BackgroundTransparency = 1.000
			Content1.Position = UDim2.new(0, 12, 0, 0)
			Content1.Size = UDim2.new(1, -12, 1, 0)
			Content1.Font = Enum.Font.GothamBold
			Content1.Text = ToggleValues.Name
			Content1.TextColor3 = Color3.fromRGB(240, 240, 240)
			Content1.TextSize = 15.000
			Content1.TextXAlignment = Enum.TextXAlignment.Left

			Frame.Parent = Toggle
			Frame.AnchorPoint = Vector2.new(0.5, 0.5)
			Frame.BackgroundColor3 = Color3.fromRGB(60, 60, 59)
			Frame.BorderSizePixel = 0
			Frame.Position = UDim2.new(1, -24, 0.5, 0)
			Frame.Size = UDim2.new(0, 24, 0, 24)

			UICorner_7.CornerRadius = UDim.new(0, 4)
			UICorner_7.Parent = Frame

			Ico.Name = "Ico"
			Ico.Parent = Frame
			Ico.AnchorPoint = Vector2.new(0.5, 0.5)
			Ico.BackgroundTransparency = 1.000
			Ico.Position = UDim2.new(0.5, 0, 0.5, 0)
			Ico.Size = UDim2.new(0, 8, 0, 8)
			Ico.Image = "rbxassetid://3944680095"
			Ico.ImageTransparency = 1.000

			TextButton.Parent = Toggle
			TextButton.BackgroundTransparency = 1.000
			TextButton.BorderSizePixel = 0
			TextButton.Size = UDim2.new(1, 0, 1, 0)
			TextButton.AutoButtonColor = false
			TextButton.Text = ""

			local function ChangeState(Bool)
				if Bool == true then
					if Toggle:FindFirstChild("Check") then
						return
					end
					local CheckFrame = Instance.new("Frame")
					CheckFrame.Position = UDim2.new(1,-24,0.5,0)
					CheckFrame.AnchorPoint = Vector2.new(0.5,0.5)
					CheckFrame.BackgroundColor3 = Color3.fromRGB(8, 99, 195)
					CheckFrame.Size = UDim2.new(0,0,0,0)
					CheckFrame.Parent = Toggle
					CheckFrame.Name = "Check"
					CheckFrame.BorderSizePixel = 0
					local CheckFrame_Icon = Instance.new("ImageLabel")
					CheckFrame_Icon.Image = "rbxassetid://3944680095"
					CheckFrame_Icon.BackgroundTransparency = 1
					CheckFrame_Icon.Parent = CheckFrame
					CheckFrame_Icon.Size = UDim2.new(0,0,0,0)
					CheckFrame_Icon.Position = UDim2.new(0.5,0,0.5,0)
					CheckFrame_Icon.AnchorPoint = Vector2.new(0.5,0.5)
					CheckFrame_Icon.BorderSizePixel = 0
					CheckFrame_Icon.Parent = CheckFrame
					local CheckFrame_UIStroke = Instance.new("UIStroke")
					CheckFrame_UIStroke.Color = Color3.fromRGB(60, 60, 59)
					CheckFrame_UIStroke.Thickness = 1
					CheckFrame_UIStroke.Parent = CheckFrame
					local CheckFrame_UICorner = Instance.new("UICorner")
					CheckFrame_UICorner.CornerRadius = UDim.new(0,4)
					CheckFrame_UICorner.Parent = CheckFrame
					local Info = TweenInfo.new(
						0.1,
						Enum.EasingStyle.Cubic,
						Enum.EasingDirection.Out,
						0,
						false,
						0
					)
					TweenService:Create(CheckFrame,Info,{Size = UDim2.new(0,24,0,24)}):Play()
					TweenService:Create(CheckFrame_Icon,Info,{Size = UDim2.new(0,20,0,20)}):Play()
				else
					if Toggle:FindFirstChild("Check") == nil then
						return
					end
					local Info = TweenInfo.new(
						0.1,
						Enum.EasingStyle.Cubic,
						Enum.EasingDirection.Out,
						0,
						false,
						0
					)
					TweenService:Create(Toggle:FindFirstChild("Check"),Info,{Size = UDim2.new(0,0,0,0)}):Play()
					TweenService:Create(Toggle:FindFirstChild("Check"):FindFirstChild("ImageLabel"),Info,{Size = UDim2.new(0,0,0,0)}):Play()
					wait(0.1)
					Toggle:FindFirstChild("Check"):Destroy()
				end
			end
			ChangeState(ToggleValues.Default)
			TextButton.MouseButton1Click:Connect(function()
				if Toggle:FindFirstChild("Check") then
					ChangeState(false)
					ToggleValues.Callback(false)
				else
					ChangeState(true)
					ToggleValues.Callback(true)
				end
			end)
			function ToggleFunctions:Set(Bool)
				ChangeState(Bool)
			end
			return ToggleFunctions
		end
		function TabFunctions:AddButton(ButtonInfo)
			local ButtonFunctions = {}

			local Button = Instance.new("Frame")
			local UICorner_6 = Instance.new("UICorner")
			local Content1 = Instance.new("TextLabel")
			local ImageLabel_2 = Instance.new("ImageLabel")
			local TextButton_3 = Instance.new("TextButton")
			local UIStroke = Instance.new("UIStroke")

			Button.Name = "Button"
			Button.Parent = ItemContainer
			Button.BackgroundColor3 = Color3.fromRGB(32, 32, 32)
			Button.BorderSizePixel = 0
			Button.Size = UDim2.new(1, 0, 0, 33)

			UICorner_6.CornerRadius = UDim.new(0, 5)
			UICorner_6.Parent = Button

			Content1.Name = "Content"
			Content1.Parent = Button
			Content1.BackgroundTransparency = 1.000
			Content1.Position = UDim2.new(0, 12, 0, 0)
			Content1.Size = UDim2.new(1, -12, 1, 0)
			Content1.Font = Enum.Font.GothamBold
			Content1.Text = ButtonInfo.Name
			Content1.TextColor3 = Color3.fromRGB(240, 240, 240)
			Content1.TextSize = 15.000
			Content1.TextXAlignment = Enum.TextXAlignment.Left

			ImageLabel_2.Parent = Button
			ImageLabel_2.BackgroundTransparency = 1.000
			ImageLabel_2.Position = UDim2.new(1, -30, 0, 7)
			ImageLabel_2.Size = UDim2.new(0, 20, 0, 20)
			ImageLabel_2.Image = "rbxassetid://3944703587"
			ImageLabel_2.ImageColor3 = Color3.fromRGB(150, 150, 150)

			TextButton_3.Parent = Button
			TextButton_3.BackgroundTransparency = 1.000
			TextButton_3.BorderSizePixel = 0
			TextButton_3.Size = UDim2.new(1, 0, 1, 0)
			TextButton_3.AutoButtonColor = false
			TextButton_3.Text = ""

			UIStroke.Parent = Button
			UIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Contextual
			UIStroke.Color = Color3.fromRGB(60, 60, 60)
			UIStroke.Thickness = 1
			TextButton_3.MouseButton1Click:Connect(function()
				ButtonInfo.Callback()
			end)
			return ButtonFunctions
		end
		function TabFunctions:AddDropdown(DropdownInfo)
			local DropdownFunctions = {}

			local DropDown = Instance.new("Frame")
			local UICorner_1 = Instance.new("UICorner")
			local ScrollingFrame = Instance.new("ScrollingFrame")
			local UIListLayout = Instance.new("UIListLayout")
			local F = Instance.new("Frame")
			local Content = Instance.new("TextLabel")
			local Ico = Instance.new("ImageLabel")
			local Selected = Instance.new("TextLabel")
			local Line = Instance.new("Frame")
			local TextButton_4 = Instance.new("TextButton")
			local UIStroke = Instance.new("UIStroke")
			--Properties:

			DropDown.Name = "DropDown"
			DropDown.Parent = ItemContainer
			DropDown.BackgroundColor3 = Color3.fromRGB(32, 32, 32)
			DropDown.BorderSizePixel = 0
			DropDown.Size = UDim2.new(1, 0, 0, 38)
			DropDown.ClipsDescendants = true

			UICorner_1.CornerRadius = UDim.new(0, 5)
			UICorner_1.Parent = DropDown

			ScrollingFrame.Parent = DropDown
			ScrollingFrame.BackgroundTransparency = 1.000
			ScrollingFrame.BorderSizePixel = 0
			ScrollingFrame.Position = UDim2.new(0, 0, 0, 39)
			ScrollingFrame.Size = UDim2.new(1, 0, 0, -38)
			ScrollingFrame.Visible = false
			ScrollingFrame.BottomImage = "rbxassetid://7445543667"
			ScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, 0)
			ScrollingFrame.AutomaticCanvasSize = Enum.AutomaticSize.Y
			ScrollingFrame.MidImage = "rbxassetid://7445543667"
			ScrollingFrame.ScrollBarThickness = 4
			ScrollingFrame.TopImage = "rbxassetid://7445543667"

			UIListLayout.Parent = ScrollingFrame
			UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder

			F.Name = "F"
			F.Parent = DropDown
			F.BackgroundTransparency = 1.000
			F.ClipsDescendants = true
			F.Size = UDim2.new(1, 0, 0, 38)

			Content.Name = "Content"
			Content.Parent = F
			Content.BackgroundTransparency = 1.000
			Content.Position = UDim2.new(0, 12, 0, 0)
			Content.Size = UDim2.new(1, -12, 1, 0)
			Content.Font = Enum.Font.MontserratBold
			Content.Text = DropdownInfo.Name
			Content.TextColor3 = Color3.fromRGB(240, 240, 240)
			Content.TextSize = 15.000
			Content.TextXAlignment = Enum.TextXAlignment.Left

			Ico.Name = "Ico"
			Ico.Parent = F
			Ico.AnchorPoint = Vector2.new(0, 0.5)
			Ico.BackgroundTransparency = 1.000
			Ico.Position = UDim2.new(1, -30, 0.5, 0)
			Ico.Size = UDim2.new(0, 20, 0, 20)
			Ico.Image = "rbxassetid://7072706796"
			Ico.ImageColor3 = Color3.fromRGB(150, 150, 150)

			UIStroke.Parent = DropDown
			UIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Contextual
			UIStroke.Color = Color3.fromRGB(60, 60, 60)
			UIStroke.Thickness = 1

			Selected.Name = "Selected"
			Selected.Parent = F
			Selected.BackgroundTransparency = 1.000
			Selected.Size = UDim2.new(1, -40, 1, 0)
			Selected.Font = Enum.Font.Gotham
			Selected.Text = "..."
			Selected.TextColor3 = Color3.fromRGB(150, 150, 150)
			Selected.TextSize = 13.000
			Selected.TextXAlignment = Enum.TextXAlignment.Right

			Line.Name = "Line"
			Line.Parent = F
			Line.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
			Line.BorderSizePixel = 0
			Line.Position = UDim2.new(0, 0, 1, -1)
			Line.Size = UDim2.new(1, 0, 0, 1)
			Line.Visible = false

			TextButton_4.Parent = F
			TextButton_4.BackgroundTransparency = 1.000
			TextButton_4.BorderSizePixel = 0
			TextButton_4.Size = UDim2.new(1, 0, 1, 0)
			TextButton_4.AutoButtonColor = false
			TextButton_4.Text = ""

			local function SelectOption(Option)
				Selected.Text = Option
				for _, i in ipairs(ScrollingFrame:GetChildren()) do
					if i:IsA("TextButton") then
						if i.Title.Text == Option then
							i.Title.TextTransparency = 0
						else
							i.Title.TextTransparency = 0.4
						end
					end
				end
				DropdownInfo.Callback(Option)
			end
			
			function DropdownFunctions:Set(Value)
				SelectOption(Value)
			end
			
			function DropdownFunctions:Refresh(Options,bool)
				if bool == true then
					for _, i in ipairs(ScrollingFrame:GetChildren()) do
						if i:IsA("TextButton") then
							i:Destroy()
						end
					end
				end
				for _, Option in ipairs(Options) do

					local TextButton_3 = Instance.new("TextButton")
					local UICorner_6 = Instance.new("UICorner")
					local Title_2 = Instance.new("TextLabel")

					TextButton_3.Parent = ScrollingFrame
					TextButton_3.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
					TextButton_3.BackgroundTransparency = 1.000
					TextButton_3.BorderSizePixel = 0
					TextButton_3.ClipsDescendants = true
					TextButton_3.Size = UDim2.new(1, 0, 0, 28)
					TextButton_3.AutoButtonColor = false
					TextButton_3.Text = ""

					UICorner_6.CornerRadius = UDim.new(0, 6)
					UICorner_6.Parent = TextButton_3

					Title_2.Name = "Title"
					Title_2.Parent = TextButton_3
					Title_2.BackgroundTransparency = 1.000
					Title_2.Position = UDim2.new(0, 8, 0, 0)
					Title_2.Size = UDim2.new(1, -8, 1, 0)
					Title_2.Font = Enum.Font.Roboto
					Title_2.Text = Option
					Title_2.TextColor3 = Color3.fromRGB(240, 240, 240)
					Title_2.TextSize = 13.000
					Title_2.TextTransparency = 0.4
					Title_2.TextXAlignment = Enum.TextXAlignment.Left

					TextButton_3.MouseButton1Click:Connect(function()
						SelectOption(Option)
					end)
				end
			end
			
			for _, Option in ipairs(DropdownInfo.Options) do

				local TextButton_3 = Instance.new("TextButton")
				local UICorner_6 = Instance.new("UICorner")
				local Title_2 = Instance.new("TextLabel")

				TextButton_3.Parent = ScrollingFrame
				TextButton_3.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
				TextButton_3.BackgroundTransparency = 1.000
				TextButton_3.BorderSizePixel = 0
				TextButton_3.ClipsDescendants = true
				TextButton_3.Size = UDim2.new(1, 0, 0, 28)
				TextButton_3.AutoButtonColor = false
				TextButton_3.Text = ""

				UICorner_6.CornerRadius = UDim.new(0, 6)
				UICorner_6.Parent = TextButton_3

				Title_2.Name = "Title"
				Title_2.Parent = TextButton_3
				Title_2.BackgroundTransparency = 1.000
				Title_2.Position = UDim2.new(0, 8, 0, 0)
				Title_2.Size = UDim2.new(1, -8, 1, 0)
				Title_2.Font = Enum.Font.Roboto
				Title_2.Text = Option
				Title_2.TextColor3 = Color3.fromRGB(240, 240, 240)
				Title_2.TextSize = 13.000
				Title_2.TextTransparency = 0.4
				Title_2.TextXAlignment = Enum.TextXAlignment.Left

				TextButton_3.MouseButton1Click:Connect(function()
					SelectOption(Option)
				end)
			end

			TextButton_4.MouseButton1Click:Connect(function()
				if DropDown.Size == UDim2.new(1,0,0,38) then
					local Size = UDim2.new(0,0,0,0)
					for _, p in ipairs(ScrollingFrame:GetChildren()) do
						if p:IsA("TextButton") then
							Size += UDim2.new(1,0,0,28)
						end
					end
					ScrollingFrame.Visible = true
					ScrollingFrame.Size = Size
					Line.Visible = true
					local Info = TweenInfo.new(
						0.1,
						Enum.EasingStyle.Cubic,
						Enum.EasingDirection.Out,
						0,
						false,
						0
					)
					TweenService:Create(DropDown,Info,{Size = UDim2.new(1,0,0,38+Size.Y.Offset)}):Play()
				else
					local Info = TweenInfo.new(
						0.1,
						Enum.EasingStyle.Cubic,
						Enum.EasingDirection.Out,
						0,
						false,
						0
					)
					TweenService:Create(DropDown,Info,{Size = UDim2.new(1,0,0,38)}):Play()
					wait(0.1)
					ScrollingFrame.Visible = false
					ScrollingFrame.Size = UDim2.new(1,0,0,0)
					Line.Visible = false
				end
			end)
			plr:GetMouse().Button1Down:Connect(function()
				local Info = TweenInfo.new(
					0.1,
					Enum.EasingStyle.Cubic,
					Enum.EasingDirection.Out,
					0,
					false,
					0
				)
				TweenService:Create(DropDown,Info,{Size = UDim2.new(1,0,0,38)}):Play()
				wait(0.1)
				ScrollingFrame.Visible = false
				ScrollingFrame.Size = UDim2.new(1,0,0,0)
				Line.Visible = false
			end)
			return DropdownFunctions
		end
		function TabFunctions:AddSection(SectionInfo)
			local SectionFunctions = {}

			local Section = Instance.new("Frame")
			local TextLabel_3 = Instance.new("TextLabel")
			local Holder = Instance.new("Frame")
			local UIListLayout_3 = Instance.new("UIListLayout")


			Section.Name = "Section"
			Section.Parent = ItemContainer
			Section.BackgroundTransparency = 1.000
			Section.Size = UDim2.new(1, 0, 0, 26)

			TextLabel_3.Parent = Section
			TextLabel_3.BackgroundTransparency = 1.000
			TextLabel_3.Position = UDim2.new(0, 0, 0, 3)
			TextLabel_3.Size = UDim2.new(1, -12, 0, 16)
			TextLabel_3.Font = Enum.Font.GothamMedium
			TextLabel_3.Text = SectionInfo.Name
			TextLabel_3.TextColor3 = Color3.fromRGB(150, 150, 150)
			TextLabel_3.TextSize = 14.000
			TextLabel_3.TextXAlignment = Enum.TextXAlignment.Left

			Holder.Name = "Holder"
			Holder.Parent = Section
			Holder.BackgroundTransparency = 1.000
			Holder.Position = UDim2.new(0, 0, 0, 23)
			Holder.Size = UDim2.new(1, 0, 1, -24)

			UIListLayout_3.Parent = Holder
			UIListLayout_3.SortOrder = Enum.SortOrder.LayoutOrder
			UIListLayout_3.Padding = UDim.new(0, 6)

			return SectionInfo
		end
		function TabFunctions:AddParagraph(Name,PContent)
			local ParagraphFunctions = {}
			local Paragraph = Instance.new("Frame")
			local UICorner = Instance.new("UICorner")
			local Title_2 = Instance.new("TextLabel")
			local Content1 = Instance.new("TextLabel")
			local UIStroke = Instance.new("UIStroke")

			Paragraph.Name = "Paragraph"
			Paragraph.Parent = ItemContainer
			Paragraph.BackgroundColor3 = Color3.fromRGB(32, 32, 32)
			Paragraph.BackgroundTransparency = 0.700
			Paragraph.BorderSizePixel = 0
			Paragraph.Size = UDim2.new(1, 0, 0, 48)

			UICorner.CornerRadius = UDim.new(0, 5)
			UICorner.Parent = Paragraph

			Title_2.Name = "Title"
			Title_2.Parent = Paragraph
			Title_2.BackgroundTransparency = 1.000
			Title_2.Position = UDim2.new(0, 12, 0, 10)
			Title_2.Size = UDim2.new(1, -12, 0, 14)
			Title_2.Text = Name
			Title_2.Font = Enum.Font.MontserratBold
			Title_2.TextColor3 = Color3.fromRGB(240, 240, 240)
			Title_2.TextSize = 15.000
			Title_2.TextXAlignment = Enum.TextXAlignment.Left

			Content1.Name = "Content"
			Content1.Parent = Paragraph
			Content1.BackgroundTransparency = 1.000
			Content1.Position = UDim2.new(0, 12, 0, 26)
			Content1.Size = UDim2.new(1, -24, 0, 13)
			Content1.AutomaticSize = Enum.AutomaticSize.Y
			Content1.Text = PContent
			Content1.TextColor3 = Color3.fromRGB(150, 150, 150)
			Content1.TextSize = 13.000
			Content1.Font = Enum.Font.MontserratMedium
			Content1.TextWrapped = true
			Content1.TextXAlignment = Enum.TextXAlignment.Left

			UIStroke.Parent = Paragraph
			UIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Contextual
			UIStroke.Color = Color3.fromRGB(60, 60, 60)
			UIStroke.Thickness = 1

			Paragraph.AutomaticSize = Enum.AutomaticSize.Y
			wait(0.1)
			Paragraph.Size += UDim2.new(0,0,0,Content1.AbsoluteSize.Y-10)
			Paragraph.AutomaticSize = Enum.AutomaticSize.None
			
			function ParagraphFunctions:Set(NewName,NewContent)
				Title_2.Text = NewName
				Content1.Text = NewContent
				Paragraph.AutomaticSize = Enum.AutomaticSize.Y
				wait(0.1)
				Paragraph.Size += UDim2.new(0,0,0,Content1.AbsoluteSize.Y-10)
				Paragraph.AutomaticSize = Enum.AutomaticSize.None
			end
			
			return ParagraphFunctions
		end
		function TabFunctions:SetTab()
			for _, Tab in ipairs(Orion:GetChildren()) do
				if Tab:IsA("ScrollingFrame") then
					Tab.Visible = false
				end
			end
			for _, Tab in ipairs(Tabs:GetChildren()) do
				if Tab:IsA("TextButton") then
					Tab.TextLabel.TextTransparency = 0.4
					Tab.TextLabel.Font = Enum.Font.MontserratMedium
					Tab.Icon.ImageTransparency = 0.4
				end
			end
			ItemContainer.Visible = true
			TabButton_Title.Font = Enum.Font.MontserratBlack
			TabButton_Title.TextTransparency = 0
			TabButton_Icon.ImageTransparency = 0
		end
		return TabFunctions
	end
	
	return WindowFunctions
end
function OrionLib:Destroy()
	OrionLib[1]:Destroy()
end
function OrionLib:Init()
	OrionLib[1].Enabled = true
end
